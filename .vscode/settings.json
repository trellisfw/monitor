{
  "search.exclude": {
    ".dccache": true,
    ".history": true,
    "node_modules": true,
    "package.lock": true,
    "*.log": true,
    ".DS_Store": true,
    "dist": true,
    ".test": true,
    "*.tsbuildinfo": true,
    ".actrc": true,
    "act": true,
    ".yarn": true,
    ".pnp.*": true,
    ".vim": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "prettier.prettierPath": ".yarn/sdks/prettier/index.cjs",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "eslint.nodePath": ".yarn/sdks",
  "files.exclude": {
    ".dccache": true,
    ".history": true,
    "node_modules": true,
    "package.lock": true,
    "*.log": true,
    ".DS_Store": true,
    "dist": true,
    ".test": true,
    "*.tsbuildinfo": true,
    ".actrc": true,
    "act": true,
    ".yarn": true,
    ".pnp.*": true,
    ".vim": true
  },
  "files.associations": {
    ".nock": "json"
  },
  "eslint.packageManager": "yarn",
  "eslint.useESLintClass": true,
  "licenser.author": "Qlever LLC",
  "licenser.license": "AL2",
  "cSpell.words": [
    "asns",
    "Ault",
    "Buildx",
    "codeql",
    "ksuid",
    "ksuids",
    "metas",
    "MONITIS",
    "mrkdwn",
    "OADA",
    "sinonjs",
    "Snyk",
    "sonarjs",
    "trellisfw"
  ],
  "npm.packageManager": "yarn",
  "debug.javascript.autoAttachFilter": "smart",
  "debug.javascript.breakOnConditionalError": true,
  "debug.javascript.defaultRuntimeExecutable": {
    "node": "yarn",
    "pwa-node": "yarn"
  },
  "debug.javascript.autoAttachSmartPattern": [
    "${workspaceFolder}/**",
    "!**/node_modules/**",
    "**/$KNOWN_TOOLS$/**",
    "!**/yarn-*",
    "!**/ava-*/**/cli.mjs"
  ]
}
